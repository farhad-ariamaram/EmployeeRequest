@page
@model EmployeeRequest.SkillPage.VersionWebsiteTablePage.CreateModel

@{
    ViewData["Title"] = "Create";
}

<h1>افزودن وبگاه ورژن</h1>

<hr />
<div class="row">
    <div class="col-md-4">
        <form method="post">
            <div asp-validation-summary="ModelOnly" class="text-danger"></div>
            <div class="form-group">
                <label asp-for="VersionWebsiteTable.VersionWebsite" class="control-label">وبگاه ورژن</label>
                <input asp-for="VersionWebsiteTable.VersionWebsite" class="form-control" />
                <span asp-validation-for="VersionWebsiteTable.VersionWebsite" class="text-danger"></span>
            </div>
            <div class="form-group">
                <label asp-for="VersionWebsiteTable.EducationalWebsite" class="control-label">وبگاه آموزشی</label>
                <input asp-for="VersionWebsiteTable.EducationalWebsite" class="form-control" />
                <span asp-validation-for="VersionWebsiteTable.EducationalWebsite" class="text-danger"></span>
            </div>
            <div class="form-group">
                <label asp-for="VersionWebsiteTable.QuestionalWebsite" class="control-label">وبگاه سوالات مرتبط</label>
                <input asp-for="VersionWebsiteTable.QuestionalWebsite" class="form-control" />
                <span asp-validation-for="VersionWebsiteTable.QuestionalWebsite" class="text-danger"></span>
            </div>
            <div class="form-group">
                <label class="control-label">ورژن</label>
                <input type="text" name="skilllid" value="@Model.VersionId" class="form-control" disabled />
                <input type="hidden" asp-for="VersionWebsiteTable.VersionId" value="@Model.VersionId" />
            </div>
            <div class="form-group">
                <input type="submit" value="Create" class="btn btn-primary" />
            </div>
        </form>
    </div>
</div>

@section Botton {
    <a class="btn btn-danger rounded-circle" asp-page="./Index" asp-route-id="@Model.VersionId"><i class="fa fa-arrow-circle-left"></i></a>
}

@section Scripts {
    @{await Html.RenderPartialAsync("_ValidationScriptsPartial");}
}
