@* filter for disapear null fields *@
<script>
    function filter() {
        if ($("#green").prop('checked')) {
            $('.bg-success').parent().show();
        } else {
            $('.bg-success').parent().hide();
        }

        if ($("#red").prop('checked')) {
            $('.bg-danger').parent().show();
        } else {
            $('.bg-danger').parent().hide();
        }

        if ($("#openall").prop('checked')) {
            $('.card-body').show();
        } else {
            $('.card-body').hide();
        }

        update($("#green").prop('checked'), $("#red").prop('checked'), $("#openall").prop('checked'));
    }

    function Load() {

        if ("@Model.TblEmployeeRequestUserSetting.FldEmployeeRequestUserSettingIsShowGreen" === "True") {
            $("#green").prop('checked', 'true');
        } else {
            $("#green").removeAttr('checked');
        }

        if ("@Model.TblEmployeeRequestUserSetting.FldEmployeeRequestUserSettingIsShowRed" === "True") {
            $("#red").prop('checked', 'true');
        } else {
            $("#red").removeAttr('checked');
        }

        if ("@Model.TblEmployeeRequestUserSetting.FldEmployeeRequestUserSettingIsCollaps" === "True") {
            $("#openall").prop('checked', 'true');
        } else {
            $("#openall").removeAttr('checked');
        }

        filter();
    }


    function update(green,red,openall) {
        var xmlHttp = new XMLHttpRequest();
        xmlHttp.open("GET", "?red=" + red + "&green=" + green + "&openall=" + openall + "&handler=Update", false);
        xmlHttp.send(null);
    }

</script>

<div class="row">
    <div class="col-12">
        <input type="checkbox" id="green" onclick="filter()" checked />  نمایش سبز
        <br />
        <input type="checkbox" id="red" onclick="filter()" checked />  نمایش قرمز
        <br />
        <input type="checkbox" id="openall" onclick="filter()" checked />  نمایش همه
    </div>
</div>
<br />

